extends layout

append content
	div.markdown
		| !{entry.content}

append script
	script(src="/public/google-code-prettify/prettify.js")
	script
		(function() {
			var pres = document.getElementsByTagName('pre');
			for (var i=0,l=pres.length; i<l; i++) {
				pres[i].className = 'prettyprint';
			}
			prettyPrint();
		})();
